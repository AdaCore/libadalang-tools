package Directions is

   type Direction_Type_2 is
     (Up,
      Down,
      Left,
      Right);

   type Direction_Type is
     (Up,     -- opposite of Down
      Down,   -- opposite of Up
      Left,   -- opposite of Right
      Right); -- opposite of Left

end Directions;
