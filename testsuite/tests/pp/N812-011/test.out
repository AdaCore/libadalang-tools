package P with
   Pure
is

   type T is new Integer with
      Atomic;

   procedure P with
      Inline;

   procedure Q (A : in Integer; B : out Positive) with
      Pre  => A > 0,
      Post => B = A'Old;

end P;
package Q is

   type T is new Integer;

   procedure P;

   procedure Q (A : in Integer; B : out Positive);

end Q;
